{
    "name": "zangsilu/yii2-gg",
    "description": "生成代码",
    "keywords": ["yii2", "gg", "code generator","代码生成"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "路小张",
            "email": "532817108@qq.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": ">=2.0.4"
    },
    "autoload": {
        "psr-4": {
            "yii\\gg\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
